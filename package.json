    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.4"
  },
  "prettier": {
    "parser": "typescript",
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "strong-events": "^1.0.7",
